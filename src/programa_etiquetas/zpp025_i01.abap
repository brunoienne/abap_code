MODULE user_command_9000 INPUT.

  CASE sy-ucomm.
    WHEN '&F03' OR '&F15' OR '&F12'.
      LEAVE TO SCREEN 0.

    WHEN 'EXECUTE'.
      go_print->co_alv->raise_event(
        EXPORTING
          i_ucomm         = '_SEND'
      ).
  ENDCASE.

ENDMODULE.                 " USER_COMMAND_9000  INPUT